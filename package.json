  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.141",
    "@types/node": "^12.7.5",
    "codecov": "^3.6.1",
    "coveralls": "^3.0.6",
    "gh-pages": "^2.1.1",
    "jest": "^24.9.0",
    "knex": "^0.19.5",
    "ts-jest": "^24.1.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  }
